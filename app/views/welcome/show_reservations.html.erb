<div class="span12">
<p>numero reservaciones salita <b><%=@reservation_for_parlo.size%></b></p>
<table class="table table-striped table-bordered table-condensed">
	<thead>
		<tr>
			<th>Reservacion</th>
			<th>Nombre Empleado</th>
			<th>Status</th>
			<th>Inicio</th>
			<th>Final</th>
			<th>Reservations</th>
		</tr>
	</thead>
	<tbody>
		<%@reservation_for_parlo.each do |reservation_parlo| %>
		<tr>
			<td><%=image_tag(reservation_parlo.employee.photo.profile)%></td>
			<td><%=reservation_parlo.employee.name%></td>
			<td><%=reservation_parlo.status%></td>
			<td><%=reservation_parlo.start_time.strftime("%I:%M %p")%></td>
			<td><%=reservation_parlo.end_time.strftime("%I:%M %p")%></td>
			<td><%=reservation_parlo.date_reservation.strftime("%d-%b-%Y")%></td>
		</tr>
		<%end%>
	</tbody>
</table>
</div>